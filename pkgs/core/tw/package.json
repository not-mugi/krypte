{
  "name": "@mugijs/tw",
  "version": "0.0.1",
  "main": "./cjs/main.js",
  "module": "./esm/main.mjs",
  "types": "./dist/bundle.d.ts",
  "files": [
    "dist",
    "esm",
    "cjs"
  ],
  "exports": {
    "." : {
      "import": {
        "types": "./lib/main.d.ts",
        "default": "./esm/main.mjs"
      },
      "require": {
        "types": "./lib/main.d.ts",
        "default": "./cjs/main.cjs"
      }
    },
    "./types" : "./dist/bundle.d.ts",
    "./mixins": {
      "import": {
        "types": "./lib/mixins/index.d.ts",
        "default": "./esm/mixins/index.mjs"
      },
      "require": {
        "types": "./lib/mixins/index.d.ts",
        "default": "./cjs/mixins/index.cjs"
      }
    }
  },
  "isCore" : true,
  "license": "Apache-2.0",
  "devDependencies": {
    "@mugijs/ts": "*"
  },
  "peerDependencies": {}
}