{
  "mainEntryPointFilePath": "lib/main.d.ts",
  "bundledPackages": ["@mugijs/ts"],
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "dist/bundle.d.ts"
  },
  "compiler": {
    "overrideTsconfig": {
      "compilerOptions": {
        "baseUrl": "../",
        "paths": {
          "@mugijs/ts/vanila": ["ts/lib/js/index.d.ts"],
          "@mugijs/ts/tailwindcss": ["ts/lib/twcss/index.d.ts"]
        }
      }
    }
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-forgotten-export": {
        "logLevel": "none"
      }
    }
  }
}
