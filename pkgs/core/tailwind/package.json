{
  "name": "@mugijs/tailwind-core",
  "version": "0.0.1",
  "main": "./cjs/index.js",
  "module": "./esm/index.mjs",
  "types": "./dist/bundle.d.ts",
  "files": [
    "dist",
    "esm",
    "cjs"
  ],
  "exports": {
