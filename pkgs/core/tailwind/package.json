{
  "name": "@mugijs/tailwind",
  "version": "0.0.1",
  "main": "./cjs/main.js",
  "module": "./esm/main.mjs",
  "types": "./dist/bundle.d.ts",
  "files": [
    "dist",
    "esm",
    "cjstailtata"
  ],
  "exports": {
    "./types" : "./lib/main.d.ts",
    "./functions": {
      "import": {
        "types": "./dist/bundle.d.ts",
        "default": "./esm/main.mjs"
      },
      "require": {
        "types": "./dist/bundle.d.ts",
        "default": "./cjs/main.cjs"
      }
    }
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@mugijs/typescript": "*"
  },
  "peerDependencies": {}
}